[sl-security]
name=SL 5 security updates
baseurl=http://ftp.scientificlinux.org/linux/scientific/57/$basearch/updates/security
		http://ftp1.scientificlinux.org/linux/scientific/57/$basearch/updates/security
		http://ftp2.scientificlinux.org/linux/scientific/57/$basearch/updates/security
		ftp://ftp.scientificlinux.org/linux/scientific/57/$basearch/updates/security
